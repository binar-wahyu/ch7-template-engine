<div class="container m-4">
  <!-- Kita lakukan loop untuk menjalankan include untuk component article -->
  <% articles.forEach(i => { %>
    <% if (i.approved) { %>
      <div class="container mb-2">
        <h1>
          <%= i.title %> <span class="badge bg-primary"><%= i.id %></span>
        </h1>
        <p><%= i.body %></p>
      </div>
    <% } %>
  <% }) %>
</div>

<script>console.log('Test')</script>

<link rel="stylesheet" href="/stylesheets/another.css" />
<style>
  body {
    background-color: red;
  }
</style>

<meta name="description" content="Test" />

<%- contentFor('customContent') %>
